{
  "name": "@julep/sdk",
  "version": "0.2.6-alpha",
  "description": "Julep is a platform for creating agents with long-term memory",
  "keywords": [
    "julep-ai",
    "julep",
    "agents",
    "llms",
    "memory",
    "ai"
  ],
  "author": {
    "name": "Julep",
    "email": "developers@julep.ai"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julep-ai/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/julep-ai/sdk/issues"
  },
  "homepage": "https://github.com/julep-ai/sdk#readme",
  "main": "dist/index.js",
  "dependencies": {
    "@types/lodash": "^4.14.202",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "openai": "^4.28.0",
    "qs": "^6.11.2",
    "rollup": "^4.13.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "url-join": "^4.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/url-join": "^4.0.3",
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.7",
    "form-data": "^4.0.0",
    "jest": "^29.7.0",
    "openapi-typescript-codegen": "^0.27.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "scripts": {
    "build": "rollup -c",
    "mock_api": "prism mock ./tests/mock_openapi.yaml -p 8080",
    "start_mock_api": "prism mock ./tests/mock_openapi.yaml -p 8080 -v fatal 1> /dev/null &",
    "kill_mock_api": "kill $(pgrep -f 'prism .*')",
    "format": "prettier -w .",
    "openapi-codegen": "openapi -c axios --name JulepApiClient --useOptions --indent 2 -i <(yq -o=json '.' ../../openapi.yaml) -o src/api --exportSchemas true --useUnionTypes true",
    "codegen": "npm run openapi-codegen && npm run format",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out ../../gitbook/js-sdk-docs src/**/*.d.ts",
    "test-inspect": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test": "npm run format && npm run start_mock_api && sleep 1 && jest && npm run kill_mock_api"
  }
}
